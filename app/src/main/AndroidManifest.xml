<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.game.happyhome.woosung">
    <uses-permission android:name="android.permission.VIBRATE" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.game.happyhome.woosung.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.game.happyhome.woosung.BombActivity"
            android:noHistory="true">></activity>
        <activity android:name="com.game.happyhome.woosung.BombSubActivity"
            android:noHistory="true">></activity>
        <activity android:name="com.game.happyhome.woosung.RandomFingerActivity"
            android:noHistory="true">></activity>
        <activity android:name="com.game.happyhome.woosung.CoinActivity"
            android:noHistory="true">></activity>
        <activity android:name="com.game.happyhome.woosung.CoinSubActivity"
            android:noHistory="true">></activity>

    </application>


</manifest>